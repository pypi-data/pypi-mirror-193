{
  "name": "feature_assembler",
  "description": "Assembles a list of numeric columns into a feature vector that can be passed to the generic classifier",
  "trainable": false,
  "output_level": "sentence",
  "outputs": [
    "feature_vector"
  ],
  "inputs": [
    "%%%feature_elements%%%"
  ],
  "type": "util",
  "spark_input_column_names": [
    "%%%feature_elements%%%"
  ],
  "spark_output_column_names": [
    "feature_vector"
  ],
  "provider": "sparknlp",
  "license": "healthcare",
  "computation_context": "spark",
  "output_context": "spark"
}