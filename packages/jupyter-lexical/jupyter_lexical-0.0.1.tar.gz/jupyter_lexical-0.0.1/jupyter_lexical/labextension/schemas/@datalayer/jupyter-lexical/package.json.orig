{
  "name": "@datalayer/jupyter-lexical",
  "version": "0.0.1",
  "description": "Jupyter React for Lexical",
  "license": "MIT",
  "private": false,
  "main": "lib/index.js",
  "files": [
    "lib",
    "style"
  ],
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "react",
    "lexical"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/datalayer/jupyter-react.git",
    "directory": "packages/lexical"
  },
  "scripts": {
    "build": "gulp resources-to-lib && tsc && webpack && jlpm build:lib && jlpm build:labextension:dev",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:prod": "gulp resources-to-lib && tsc && jlpm clean && jlpm build:lib && jlpm build:labextension",
    "build:tsc:watch:res": "gulp resources-to-lib-watch",
    "build:tsc:watch:tsc": "tsc --watch",
    "clean": "rimraf lib build dist tsconfig.tsbuildinfo",
    "clean:all": "jlpm clean:lib && jlpm clean:labextension && jlpm clean:lintcache",
    "clean:labextension": "rimraf datalayer/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:lintcache": "rimraf .eslintcache .stylelintcache",
    "eslint": "jlpm eslint:check --fix",
    "eslint:check": "eslint . --cache --ext .ts,.tsx",
    "install:extension": "jlpm build",
    "jupyterlab": "run-p -c jupyterlab:*",
    "jupyterlab:start": "jupyter lab --watch --config=./dev/config/jupyter_server_config.py",
    "jupyterlab:watch": "run-p -c watch:src watch:labextension",
    "kill": "./../../dev/sh/kill-jupyter-server.sh || true ",
    "lint": "jlpm stylelint && jlpm prettier && jlpm eslint",
    "lint:check": "jlpm stylelint:check && jlpm prettier:check && jlpm eslint:check",
    "prettier": "jlpm prettier:base --write --list-different",
    "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "prettier:check": "jlpm prettier:base --check",
    "start": "run-p -c start:*",
    "start:jupyter": "cd ./../.. && make start-jupyter-server",
    "start:webpack": "webpack serve",
    "stylelint": "jlpm stylelint:check --fix",
    "stylelint:check": "stylelint --cache \"style/**/*.css\"",
    "test": "jest --coverage",
    "typedoc": "typedoc ./src",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "dependencies": {
    "@datalayer/jupyter-react": "0.2.2",
    "@lexical/react": "0.5.0",
    "@jupyterlab/application": "4.0.0-alpha.19",
    "@jupyterlab/settingregistry": "4.0.0-alpha.19",
    "@jupyterlab/coreutils": "6.0.0-alpha.19",
    "@jupyterlab/services": "7.0.0-alpha.19",
    "@primer/react": "35.18.0",
    "@primer/octicons-react": "17.10.0",
    "@datalayer/icons": "0.0.10",
    "styled-components": "4.4.1",
    "katex": "0.16.1",
    "lexical": "0.5.0",
    "lodash-es": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0"
   },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@jupyterlab/builder": "4.0.0-alpha.19",
    "@types/codemirror": "5.60.4",
    "@types/katex": "0.14.0",
    "@types/lodash-es": "4.17.6",
    "@types/marked": "4.0.1",
    "@types/node": "18.11.9",
    "@types/plotly.js": "1.54.11",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/styled-components": "5.1.26",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "@typescript-eslint/parser": "4.8.1",
    "babel-loader": "8.3.0",
    "bundle-loader": "0.5.6",
    "css-loader": "5.2.4",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "file-loader": "~5.0.2",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "gulp-filter": "6.0.0",
    "gulp-watch": "5.0.1",
    "html-webpack-plugin": "5.3.1",
    "html-webpack-tags-plugin": "2.0.17",
    "jest": "26.0.0",
    "mkdirp": "1.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.1",
    "process": "^0.11.10",
    "raw-loader": "~4.0.0",
    "rimraf": "~3.0.0",
    "stream-browserify": "2.0.2",
    "style-loader": "2.0.0",
    "stylelint": "14.3.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-recommended": "6.0.0",
    "stylelint-config-standard": "~24.0.0",
    "stylelint-prettier": "2.0.0",
    "svg-url-loader": "~3.0.3",
    "typedoc": "0.20.37",
    "typescript": "4.8.1-rc",
    "url-loader": "~3.0.0",
    "watch": "~1.0.2",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.3"
  },
  "sideEffects": [
    "style/*.css",
    "src/**/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "discovery": {
        "server": {
          "managers": [
            "pip"
          ],
          "base": {
            "name": "jupyter_lexical"
          }
        }
    },
    "extension": "./lib/jupyter/lab/index.js",
    "outputDir": "jupyter_lexical/labextension",
    "schemaDir": "schema"
  },
  "jupyter-releaser": {
    "hooks": {
      "before-build-npm": [
        "python -m pip install jupyterlab==4.0.0a34",
        "jlpm"
      ],
      "before-build-python": [
        "jlpm clean:all"
      ]
    }
  }
}
